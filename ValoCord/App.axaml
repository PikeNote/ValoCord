<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:ValoCord"
             x:Class="ValoCord.App"
             xmlns:sty = "using:FluentAvalonia.Styling"
             RequestedThemeVariant="Dark">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <sty:FluentAvaloniaTheme PreferUserAccentColor="True"></sty:FluentAvaloniaTheme>
        <FluentTheme/>
    </Application.Styles>
</Application>
